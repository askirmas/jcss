{
  "$schema": "./doctest.schema.json",
  "Empties": [
    ["UNST", "Empty source", [
      [],
      [{}]
    ]],
    ["UNST", "Empty property", [
      [],
      [{"empty": {}}]
    ]],
    ["UNST", "Empty property^2", [
      [],
      [{"empty": {"empty": {}}}]
    ]],
    ["UNST", "Empty property", [
      [],
      [{"empty": {}}]
    ]],
    ["UNST", "Empty properties", [
      [],
      [{"empty": {}, "empty1": {"empty": {}}}]
    ]]

  ],
  "Flats": [
    ["UNST", "1 prop", [
      [["prop", "x"]],
      [{"prop": "x"}]
    ]],
    ["UNST", "scalars check", [
      [
        ["0", 0],
        ["null", null],
        ["false", false],
        ["string", "string"],
        ["empty array", []],
        ["not empty array", ["empty array"]]
      ],
      [{
        "null": null,
        "false": false,
        "0": 0,
        "string": "string",
        "empty array": [],
        "not empty array": ["empty array"]
      }]
    ]]
  ],
  "Nested": [
    ["UNST", "1", [
      [
        ["border-top-width", [1, "px"]],
        ["border-style", "dashed"]
      ],
      [{
        "border": {
          "top": {
            "width": [1, "px"]
          },
          "style": "dashed"
        }
      }]
    ]],
    ["UNST", "Preserve order", [
      [
        ["a", 0],
        ["b-c", "2"],
        ["b-d-e", null],
        ["b-f", [4]],
        ["g", 5]
      ],
      [{
        "a": 0,
        "b": {
          "c": "2",
          "d": {
            "e": null
          },
          "f": [4]
        },
        "g": 5
      }]
    ]]
  ],
  "Special": [
    ["UNST", "@import", [
      [["@import", ["example.com", "xx"]]],
      [{"@import": ["example.com", "xx"]}]
    ]],
    ["UNST", "@include", [
      [
        ["@include", [
          {
            "myFunc": ["arg1", "arg2"],
            "myAnotherFunction": "x"
          },
          "my3rdFunction",
          {"myLastFunction": null}
        ]]
      ],
      [{
        "@include": [
          {
            "myFunc": ["arg1", "arg2"],
            "myAnotherFunction": "x"
          },
          "my3rdFunction",
          {"myLastFunction": null}
        ]
      }]
    ]]
  ],
  "Options": [
    ["UNST", "Default `\"propSpace\": \"\"`", [
      [
        ["left", "50px"],
        ["height", "50px"]
      ],
      [
        {
          "left": "50px",
          "height": "50px"
        },
        {"propSpace": ""}
      ]
    ]],
    ["UNST", "`\"propSpace\": \"padding\"`", [
      [
        ["padding-left", "50px"],
        ["padding-height", "50px"]
      ],
      [
        {
          "left": "50px",
          "height": "50px"
        },
        {"propSpace": "padding"}
      ]
    ]],
    ["UNST", "Or variable `\"propSpace\": \"$rect\"`", [
      [
        ["$rect-left", "50px"],
        ["$rect-height", "50px"]
      ],
      [
        {
          "left": "50px",
          "height": "50px"
        },
        {"propSpace": "$rect"}
      ]
    ]]
  ],
  "Weird": [
    ["UNST", "Delimiter underscore `\"propDelimiter\": \"_\"`", [
      [
        ["my_left", "50px"],
        ["my_height", "50px"]
      ],
      [
        {"my": {
          "left": "50px",
          "height": "50px"
        }},
        {"propDelimiter": "_"}
      ]
    ]],
    ["BUG+", "Empty underscore `\"empty\": \"_\"`", [
      [
        ["--_my-left", "50px"],
        ["--_my-height", "50px"]
      ],
      [
        {"my": {
          "left": "50px",
          "height": "50px"
        }},
        {"empty": "_"}
      ]
    ]]    

  ]
}