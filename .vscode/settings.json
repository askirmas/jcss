{
  "json.schemas": [
    {
      "fileMatch": [
        "jest.config.json"
      ],
      "url": "https://askirmas.github.io/jest.schema.json"
    },
    {
      "fileMatch": [
        "*.schema.*json",
        "schema.json"
      ],
      "url": "http://json-schema.org/draft-07/schema"
    },
    {
      "fileMatch": [
        "*.doctest.*json"
      ],
      "url": "./doctest/schema.json"
    },
    {
      "fileMatch": [
        "function-schema.json",
        "*.function-schema.*json"
      ],
      "url": "./doctest/function.schema.json"
    },
    {
      "fileMatch": [
        "*.jcss",
        "*.jcss.*json"
      ],
      "url": "./jcss.schema.json"
    }
  ]
}